<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <servlet>
    <servlet-name>register</servlet-name>
    <servlet-class>org.example.Frontend.RegisterServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>register</servlet-name>
    <url-pattern>/register</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>post-request</servlet-name>
    <servlet-class>org.example.Backend.BackendServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>post-request</servlet-name>
    <url-pattern>/back</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>main-window</servlet-name>
    <servlet-class>org.example.Frontend.Main</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>main-window</servlet-name>
    <url-pattern>/main</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>login</servlet-name>
    <servlet-class>org.example.Frontend.LoginServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>login</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>settings</servlet-name>
    <servlet-class>org.example.Frontend.SettingsServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>settings</servlet-name>
    <url-pattern>/settings</url-pattern>
  </servlet-mapping>
</web-app>
